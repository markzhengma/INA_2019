{
  "manifest_version": 2,
  "name": "pdf-detector",
  "description": "detect pdf and html...",
  "version": "1.0",
  "browser_action": {},
  "permissions": [
  ],
  "background": {
    "scripts": ["eventPage.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"]
    }
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
